<style>
  @import url("https://fonts.googleapis.com/css2?family=Playfair+Display:ital,wght@0,400;0,500;0,600;0,700;0,800;0,900;1,400;1,500;1,600;1,700;1,800;1,900&family=Playfair:ital,opsz,wght@0,5..1200,300;0,5..1200,400;0,5..1200,500;0,5..1200,600;0,5..1200,700;0,5..1200,800;0,5..1200,900;1,5..1200,300;1,5..1200,400;1,5..1200,500;1,5..1200,600;1,5..1200,700;1,5..1200,800;1,5..1200,900&family=Poppins:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,400;1,500;1,600;1,700;1,800;1,900&display=swap");

  .cartHide {
    display: none;
  }
  .cartShow {
    display: block !important;
  }
  .hidePhone {
    display: none;
  }
  .hidePhone:hover {
    display: block;
  }
  .callIcon:hover + .hidePhone {
    display: block;
  }
  .searchBox {
    display: none;
  }
  .searchIcon:hover + .searchBox {
    display: block;
  }
  .searchBox:hover {
    display: block;
  }
  .search-list-items {
    display: none;
    max-height: 500px;
    overflow-y: scroll;
  }
  ul.search-lists-loop {
    background: #fff;
    padding: 20px 10px;
  }
  ul.search-lists-loop li a {
    display: flex;
    text-align: start;
    text-align: left;
  }
  ul.search-lists-loop li a img {
    width: 50px;
    height: 50px;
    object-fit: cover;
    margin-right: 10px;
  }
  ul.search-lists-loop li {
    margin-bottom: 8px;
  }
  .product-info {
    margin-left: 10px;
    font-size: 14px;
  }
  .blogMenu{
    position: relative;
    padding: 20px 0px;
  }
  .blogMenu a{
    font-family: "Playfair Display", serif;
    font-style: italic;
    font-weight: 600;
    font-size: 25px !important;
    color: #EA5329;
  }
  .discoverSub{
    position: absolute;
    top: 55px;
    left: -5px;
    background: #fff;
    width: 160px;
    padding: 10px 10px 10px 20px;
    border-radius: 5px;
    display: none;
    z-index: 999;
  }
  .discoverSub ul li a{
    font-family: "Lato, Antiqua, sans-serif";
    font-style: normal;
    font-weight: 500;
    color: #3b1408;
    font-size: 18px !important;
    line-height: 2;
  }
  .blogMenu:hover .discoverSub{
    display: block;
  }
</style>
<div class="header">
  <div class=" bg-primary py-2 laptop:px-4">
    <div class="m-auto tablet:px-3 container">
      <div class="flex gap-4 justify-between">
        <div>
          <div id="google_translate_element">
            <div class="skiptranslate goog-te-gadget" dir="ltr" style="">
              <div id=":0.targetLanguage">
                <select
                  class="goog-te-combo bg-transparent text-white"
                  aria-label="Language Translate Widget"
                  onchange="window.location.href=this.value"
                >
                  <option value="">Select Language</option>
                  <option value="/">English</option>
                  <option value="fr">French</option>
                </select>
              </div>
            </div>
          </div>
        </div>
        <div class="">
          <p class="text-white text-[16px] phone:text-[14px]">{{ section.settings.infoTopText }}</p>
        </div>
        <div class="hidden">
          <select class="text-[14px] bg-transparent text-white" name="usd" id="usd">
            <option value="usd">USD</option>
            <option value="cad">CAD</option>
          </select>
        </div>
      </div>
    </div>
  </div>
  <div class="relative container m-auto tablet:px-3 laptop:px-4">
    <div class="flex justify-between items-center">
      <div class="cursor-pointer">
        <a href="/">
          {% if section.settings.logo != blank %}
            <img
              alt="logo"
              class="w-[90px] h-[90px]"
              src="{{ section.settings.logo | img_url: '' }}"
              width=""
              height=""
              loading="eager"
            >
          {% else %}
            <img
              alt="logo"
              src="{{ 'logo.svg' | asset_url }}"
              style="display: block; max-width: 100%; width: initial; height: initial; background: none; opacity: 1; border: 0px; margin: 0px; padding: 0px;"
              width=""
              height=""
              loading="eager"
            >
          {% endif %}
        </a>
      </div>
      <div class="relative main-menu tablet:hidden">
        <ul class="flex gap-12 miniLaptop:gap-5 h-[96px] items-center">
          <li class="cursor-pointer"><a href="/collections/all">All Chocolates</a></li>
          <li class="cursor-pointer group-hover group py-[48px]">
            <a>Signature Collections</a>
            <div
              class="hidden group-hover:block hover:block left-[35%] transform -translate-x-1/2 top-[96px] bg-white/70 pt-8 pb-14 px-12 mt-0 justify-start gap-10 overflow-hidden absolute z-50 bg-auto bg-no-repeat bg-right-top m-auto w-auto max-w-full container tablet:px-3"
            >
              <div class="flex gap-[50px]">
                {% for link in linklists['header-menu-signature-collections'].links %}
                  {% assign collection = link.object %}
                  <div class="inline-block">
                    <a href="{{ collection.url }}">
                      <div class="border-2 border-secondary rounded-full overflow-hidden w-[100px] h-[100px]">
                        <span
                          style="box-sizing: border-box; display: inline-block; overflow: hidden; width: 100px; height: 100px; background: none; opacity: 1; border: 0px; margin: 0px; padding: 0px; position: relative;"
                          ><img
                            src="{{ collection.image.src | img_url: 'medium' }}"
                            style="position: absolute; inset: 0px; box-sizing: border-box; padding: 0px; border: none; margin: auto; display: block; width: 0px; height: 0px; min-width: 100%; max-width: 100%; min-height: 100%; max-height: 100%;"
                            width=""
                            height=""
                            loading="eager"
                        ></span>
                      </div>
                    </a>
                    <h3 class="text-center mt-2 hover:text-primary">
                      <a href="{{ collection.url }}">{{ collection.title }}</a>
                    </h3>
                  </div>
                {% endfor %}
              </div>
              <div class="absolute right-0 bottom-0">
                <a href="/collections/{{ section.settings.signature_collections }}">
                  <button
                    class="bg-primary py-3 px-7 text-white hover:text-white hover:bg-primary/95"
                  >
                    Shop All
                  </button>
                </a>
              </div>
            </div>
          </li>
          <li class="cursor-pointer group-hover group py-[48px]">
            <a>Party Platters</a>
            <div
              class="hidden group-hover:block hover:block left-1/2 transform -translate-x-1/2 bg-white/70 top-[96px] pt-8 pb-14 px-12 mt-0 justify-start gap-10 overflow-hidden absolute z-50 bg-auto bg-no-repeat bg-right-top m-auto w-auto max-w-full container tablet:px-3"
            >
              <div class="flex gap-[50px]">
                {% for link in linklists['header-menu-party-platters'].links %}
                  {% assign collection = link.object %}
                  <div class="inline-block">
                    <a href="{{ collection.url }}">
                      <div class="border-2 border-secondary rounded-full overflow-hidden w-[100px] h-[100px]">
                        <span
                          style="box-sizing: border-box; display: inline-block; overflow: hidden; width: 100px; height: 100px; background: none; opacity: 1; border: 0px; margin: 0px; padding: 0px; position: relative;"
                          ><img
                            src="{{ collection.image.src | img_url: 'medium' }}"
                            style="position: absolute; inset: 0px; box-sizing: border-box; padding: 0px; border: none; margin: auto; display: block; width: 0px; height: 0px; min-width: 100%; max-width: 100%; min-height: 100%; max-height: 100%;"
                            width=""
                            height=""
                            loading="eager"
                        ></span>
                      </div>
                    </a>
                    <h3 class="text-center mt-2 hover:text-primary">
                      <a href="{{ collection.url }}">{{ collection.title }}</a>
                    </h3>
                  </div>
                {% endfor %}
              </div>
              <div class="absolute right-0 bottom-0">
                <a href="/collections/{{ section.settings.party_collections }}">
                  <button
                    class="bg-primary py-3 px-7 text-white hover:text-white hover:bg-primary/95"
                  >
                    Shop All
                  </button>
                </a>
              </div>
            </div>
          </li>
          <li class="cursor-pointer group-hover group py-[48px]">
            <a>Gourmet Gifts</a>
            <div
              class="hidden group-hover:block hover:block left-1/2 transform -translate-x-1/2 top-[96px] bg-white/70 mt-0 justify-start gap-10 overflow-hidden absolute z-50 bg-auto bg-no-repeat bg-right-top m-auto container tablet:px-3"
            >
              <div class="grid grid-cols-7">
                <div class="relative col-span-5 pt-8 pb-14 px-8 ">
                  <div class="grid grid-cols-3 gap-3 text-center">
                    {% for link in linklists['header-menu-gourmet-gifts'].links %}
                      {% assign collection = link.object %}
                      <div class="inline-block text-center">
                        <a href="{{ collection.url }}">
                          <div
                            class="border-2 border-secondary rounded-full overflow-hidden w-[100px] h-[100px] inline-block"
                          >
                            <span
                              style="box-sizing: border-box; display: inline-block; overflow: hidden; width: 100px; height: 100px; background: none; opacity: 1; border: 0px; margin: 0px; padding: 0px; position: relative;"
                              ><img
                                src="{{ collection.image.src | img_url: 'medium' }}"

                                style="position: absolute; inset: 0px; box-sizing: border-box; padding: 0px; border: none; margin: auto; display: block; width: 0px; height: 0px; min-width: 100%; max-width: 100%; min-height: 100%; max-height: 100%;"
                                width=""
                                height=""
                                loading="eager"
                              >
                            </span>
                          </div>
                        </a>
                        <h3 class="text-center mt-2 hover:text-primary">
                          <a href="{{ collection.url }}">{{ collection.title }}</a>
                        </h3>
                      </div>
                    {% endfor %}
                  </div>
                  <div class="absolute right-0 bottom-0">
                    <a href="/collections/{{ section.settings.gift_collections }}">
                      <button
                        class="bg-primary py-3 px-7 text-white hover:text-white hover:bg-primary/95"
                      >
                        Shop All
                      </button>
                    </a>
                  </div>
                </div>
                <div class="bg-[#FAFAFA]/70 col-span-2 text-center flex items-center justify-center">
                  <a href="{{ section.settings.customize_url }}">
                    <div>
                      <h4 class="font-bold hover:text-primary">Customize Your Box!</h4>
                      <div class="inline-block mt-3 group">
                        <div
                          class="border-2 border-secondary rounded-sm overflow-hidden w-[100px] h-[100px] text-center m-auto"
                        >
                          <span
                            style="box-sizing: border-box; display: inline-block; overflow: hidden; width: 100px; height: 100px; background: none; opacity: 1; border: 0px; margin: 0px; padding: 0px; position: relative;"
                            ><img
                              src="{{ section.settings.customize_image | img_url: 'medium' }}"
                              style="position: absolute; inset: 0px; box-sizing: border-box; padding: 0px; border: none; margin: auto; display: block; width: 0px; height: 0px; min-width: 100%; max-width: 100%; min-height: 100%; max-height: 100%;"
                              width=""
                              height=""
                              loading="eager"
                          ></span>
                        </div>
                        <h3 class="text-center mt-2 hover:text-primary">Many Color Options!</h3>
                      </div>
                    </div>
                  </a>
                </div>
              </div>
            </div>
          </li>
          {% comment %} <li class="cursor-pointer"><a href="/pages/contact">Contact</a></li> {% endcomment %}
          <li class="cursor-pointer blogMenu"><a href="#">Discover</a>
            <div class="discoverSub">
              <ul>
                {% comment %} <li><a href="#">About Us</a></li> {% endcomment %}
                {% comment %} <li><a href="#">Events</a></li> {% endcomment %}
                {% comment %} <li><a href="#">Our Flavours</a></li> {% endcomment %}
                <li><a href="/blogs/discover">Blog</a></li>
                <li><a href="/pages/discover-our-flavours">Our Flavors</a></li>
              </ul>
            </div>
          </li>
        </ul>
      </div>
      <div class="relative ">
        <div class="flex items-center gap-8 phone:gap-5">
          <div class="phone:hidden tablet:hidden">
            <div class="callIcon">
              <svg
                stroke="currentColor"
                fill="currentColor"
                stroke-width="0"
                viewBox="0 0 24 24"
                class="icons"
                height="1em"
                width="1em"
                xmlns="http://www.w3.org/2000/svg"
              >
                <path fill="none" d="M0 0h24v24H0V0z"></path>
                <path
                  d="M6.54 5c.06.89.21 1.76.45 2.59l-1.2 1.2c-.41-1.2-.67-2.47-.76-3.79h1.51m9.86 12.02c.85.24 1.72.39 2.6.45v1.49c-1.32-.09-2.59-.35-3.8-.75l1.2-1.19M7.5 3H4c-.55 0-1 .45-1 1 0 9.39 7.61 17 17 17 .55 0 1-.45 1-1v-3.49c0-.55-.45-1-1-1-1.24 0-2.45-.2-3.57-.57a.84.84 0 00-.31-.05c-.26 0-.51.1-.71.29l-2.2 2.2a15.149 15.149 0 01-6.59-6.59l2.2-2.2c.28-.28.36-.67.25-1.02A11.36 11.36 0 018.5 4c0-.55-.45-1-1-1z">
                </path>
              </svg>
            </div>

            <div class="hidePhone tablet:hidden">
              <div
                class=" bg-white mt-0 text-center w-[400px] tablet:w-full flex flex-col items-center justify-center content-center bg-no-repeat bg-right h-[450px] absolute right-0 divide-y divide-solid p-[75px] tablet:px-3 shadow-sm rounded-sm"
                style="background-image: url({{ 'menu-bg.png' | asset_url }})"
              >
                <div class="w-full pb-8 border-b-1 border-b-gray-500">
                  <p class="text-[30px] font-normal mb-6 font-title">{{ section.settings.phone }}</p>
                  <a href="tel:{{ section.settings.email }}" class="button !py-3 w-full !px-7 cursor-pointer"
                    >Call Now</a
                  >
                </div>
                <div class="w-full pt-4">
                  <p class="text-[30px] font-normal mb-6 font-title">{{ section.settings.email }}</p>
                  <a href="mailto:{{ section.settings.email }}" class="button w-full !py-3 !px-7 cursor-pointer"
                    >Send Us And Email</a
                  >
                </div>
              </div>
            </div>
          </div>

          <div class="phone:hidden tablet:hidden">
            <div class="searchIcon">
              <svg
                stroke="currentColor"
                fill="currentColor"
                stroke-width="0"
                viewBox="0 0 24 24"
                class="icons"
                height="1em"
                width="1em"
                xmlns="http://www.w3.org/2000/svg"
              >
                <path
                  d="M10 18a7.952 7.952 0 0 0 4.897-1.688l4.396 4.396 1.414-1.414-4.396-4.396A7.952 7.952 0 0 0 18 10c0-4.411-3.589-8-8-8s-8 3.589-8 8 3.589 8 8 8zm0-14c3.309 0 6 2.691 6 6s-2.691 6-6 6-6-2.691-6-6 2.691-6 6-6z">
                </path>
              </svg>
            </div>

            <div class="searchBox">
              <div
                class="  mt-0 text-center w-[400px] tablet:w-full flex flex-col items-start justify-start content-center bg-no-repeat bg-right h-auto absolute right-0 divide-y divide-solid p-[10px] tablet:px-3 shadow-sm rounded-sm"
              >
                <div class="search-form w-full">
                  <form action="{{ routes.search_url }}">
                    <div class="w-full">
                      <input
                        id="search-box"
                        class="form-control w-full h-[50px] shadow-md px-3 border-2px border-gray-300 rounded-md"
                        name="q"
                        type="search"
                        value="{{ search.terms | escape }}"
                        placeholder="Search..."
                        autocomplete="off"
                      >
                    </div>

                    <div class="search-list-items">
                      <ul class="search-lists-loop"></ul>
                      {% comment %} <button class="view-all" type="submit">View All</button> {% endcomment %}
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>

          {% comment %}
            <div class="relative">
              <a href="#!" class="cart-buttons">
                <p class="absolute -top-4 -right-3 bg-primary/20 h-6 w-6 rounded-full text-center font-bold text-primary">
                  0
                </p>
                <svg
                  stroke="currentColor"
                  fill="currentColor"
                  stroke-width="0"
                  viewBox="0 0 512 512"
                  class="icons"
                  height="1em"
                  width="1em"
                  xmlns="http://www.w3.org/2000/svg"
                >
                  <path
                    d="M458.4 64.3C400.6 15.7 311.3 23 256 79.3 200.7 23 111.4 15.6 53.6 64.3-21.6 127.6-10.6 230.8 43 285.5l175.4 178.7c10 10.2 23.4 15.9 37.6 15.9 14.3 0 27.6-5.6 37.6-15.8L469 285.6c53.5-54.7 64.7-157.9-10.6-221.3zm-23.6 187.5L259.4 430.5c-2.4 2.4-4.4 2.4-6.8 0L77.2 251.8c-36.5-37.2-43.9-107.6 7.3-150.7 38.9-32.7 98.9-27.8 136.5 10.5l35 35.7 35-35.7c37.8-38.5 97.8-43.2 136.5-10.6 51.1 43.1 43.5 113.9 7.3 150.8z">
                  </path>
                </svg>
              </a>
            </div>
          {% endcomment %}
          <div class="relative cartTab" id="cartTabs">
            <p class="absolute -top-4 -right-3 bg-primary/20 h-6 w-6 rounded-full text-center font-bold text-primary">
              {{ cart.item_count }}
            </p>
            <svg
              stroke="currentColor"
              fill="currentColor"
              stroke-width="0"
              viewBox="0 0 24 24"
              class="icons"
              height="1em"
              width="1em"
              xmlns="http://www.w3.org/2000/svg"
            >
              <g>
                <path fill="none" d="M0 0h24v24H0z"></path>
                <path
                  d="M4 16V4H2V2h3a1 1 0 0 1 1 1v12h12.438l2-8H8V5h13.72a1 1 0 0 1 .97 1.243l-2.5 10a1 1 0 0 1-.97.757H5a1 1 0 0 1-1-1zm2 7a2 2 0 1 1 0-4 2 2 0 0 1 0 4zm12 0a2 2 0 1 1 0-4 2 2 0 0 1 0 4z">
                </path>
              </g>
            </svg>
          </div>
        </div>
      </div>
      <div class="group relative hidden tablet:block tablet:px-3" onclick="mobileMenu()" id="menuIcon">
        <svg
          stroke="currentColor"
          fill="currentColor"
          stroke-width="0"
          viewBox="0 0 12 16"
          class="text-primary"
          height="32"
          width="32"
          xmlns="http://www.w3.org/2000/svg"
        >
          <path fill-rule="evenodd"
            d="M11.41 9H.59C0 9 0 8.59 0 8c0-.59 0-1 .59-1H11.4c.59 0 .59.41.59 1 0 .59 0 1-.59 1h.01zm0-4H.59C0 5 0 4.59 0 4c0-.59 0-1 .59-1H11.4c.59 0 .59.41.59 1 0 .59 0 1-.59 1h.01zM.59 11H11.4c.59 0 .59.41.59 1 0 .59 0 1-.59 1H.59C0 13 0 12.59 0 12c0-.59 0-1 .59-1z">
          </path>
        </svg>
      </div>
    </div>
    <div
      style="display: none;"
      id="mobileMenu"
      class="mobile-menu absolute bg-white left-1/2 transform -translate-x-1/2 w-[96%] py-8 px-8 rounded-md mt-0 shadow-lg display-none"
    >
      <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/collections/all">Shop</a></li>
        <li><a href="/blogs/news">Blogs</a></li>
        <li><a href="/pages/contact">Contact Us</a></li>
      </ul>
    </div>
  </div>
</div>

<div class="w-[300px] relative z-50" id="catContainer">
  <div class="cartHide">
    <form action="/cart" method="post" id="cartUpdate" novalidate>
      <div class="bg-white cartBoxShadow w-[300px] h-[100vh] fixed right-0 top-0 p-2 flex flex-col justify-between">
        <div class="">
          <div class="cartTab">
            <svg
              stroke="currentColor"
              fill="currentColor"
              stroke-width="0"
              viewBox="0 0 1024 1024"
              class="text-secondary ml-0 mt-2 cursor-pointer"
              height="30"
              width="30"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path d="M512 0C229.232 0 0 229.232 0 512c0 282.784 229.232 512 512 512 282.784 0 512-229.216 512-512C1024 229.232 794.784 0 512 0zm0 961.008c-247.024 0-448-201.984-448-449.01 0-247.024 200.976-448 448-448s448 200.977 448 448-200.976 449.01-448 449.01zm181.008-630.016c-12.496-12.496-32.752-12.496-45.248 0L512 466.752l-135.76-135.76c-12.496-12.496-32.752-12.496-45.264 0-12.496 12.496-12.496 32.752 0 45.248L466.736 512l-135.76 135.76c-12.496 12.48-12.496 32.769 0 45.249 12.496 12.496 32.752 12.496 45.264 0L512 557.249l135.76 135.76c12.496 12.496 32.752 12.496 45.248 0 12.496-12.48 12.496-32.769 0-45.249L557.248 512l135.76-135.76c12.512-12.512 12.512-32.768 0-45.248z"></path>
            </svg>
          </div>
          <div>
            {% if cart.item_count != 0 %}
              <h2 class="mt-5 text-secondary content font-bold">Product Details</h2>
            {% else %}
              <h2 class="mt-5 text-secondary content font-bold">Cart is empty!</h2>
            {% endif %}
            {% for item in cart.items %}
              <div class="mt-7 flex items-center justify-start gap-2 rounded-lg p-1 relative">
                <div
                  data-line="{{ forloop.index }}"
                  class="removeProduct"
                >
                  <svg
                    stroke="currentColor"
                    fill="currentColor"
                    stroke-width="0"
                    viewBox="0 0 24 24"
                    class="text-secondary ml-0 mt-2 absolute right-1 top-0 cursor-pointer"
                    height="20"
                    width="20"
                    xmlns="http://www.w3.org/2000/svg"
                  >
                    <g><path fill="none" d="M0 0h24v24H0z"></path><path d="M12 10.586l4.95-4.95 1.414 1.414-4.95 4.95 4.95 4.95-1.414 1.414-4.95-4.95-4.95 4.95-1.414-1.414 4.95-4.95-4.95-4.95L7.05 5.636z"></path></g>
                  </svg>
                </div>

                <span style="box-sizing: border-box; display: inline-block; overflow: hidden; width: 65px; height: 65px; background: none; opacity: 1; border: 0px; margin: 0px; padding: 0px; position: relative;">
                  <img
                    alt="--"
                    src="{{ item | img_url: '' }}"
                    style="position: absolute; inset: 0px; box-sizing: border-box; padding: 0px; border: none; margin: auto; display: block; width: 0px; height: 0px; min-width: 100%; max-width: 100%; min-height: 100%; max-height: 100%; object-fit: cover;"
                    width=""
                    height=""
                    loading="eager"
                ></span>
                <div>
                  <p class="text-secondary text-[13px] font-title font-bold mb-1 pr-4">
                    <a href="{{ item.url }}">{{ item.product.title }}</a>
                  </p>
                  <p class="text-secondary text-[12px] font-title">{{ item.quantity }} X {{ item.price | money }}</p>
                </div>
              </div>
            {% endfor %}
          </div>
        </div>
        <div class="border-t pt-3 text-center">
          <h2 class="text-secondary text-[20px]">
            Sub Total:
            {{- cart.currency.symbol -}}
            {{- cart.total_price | money_without_currency -}}
          </h2>
          <div class="flex justify-center items-center py-3">
            <a href="/cart" class="bg-secondary text-white py-3 px-5 rounded-xl font-bold w-full">View Cart</a>
          </div>
        </div>
      </div>
    </form>
  </div>
</div>
